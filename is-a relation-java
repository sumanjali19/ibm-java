// Example of "is-a" relationship in java
class Animal {
    void eat() {
        System.out.println("Animal is eating");
    }
}

class Dog extends Animal {
    void bark() {
        System.out.println("Dog is barking");
    }
}

public class Main {
    public static void main(String[] args) {
        Dog myDog = new Dog();
        myDog.eat(); // Accessing eat() method from Animal class
        myDog.bark(); // Accessing bark() method from Dog class
    }
}
